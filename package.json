{
  "name": "connect",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "///////////////////////////////////////////        start          //////////////////////////////////////": "",
    "start": "concurrently --raw --kill-others \"npm run apiProxy\" \"webpack-dev-server --config webpack.dev.js --open\"",
    "//////////////////////////////////////        build         ////////////////////////////////////////": "",
    "build": "webpack --config webpack.prod.js --progress --hide-modules",
    "//////////////////////////////////////         lint:js          ////////////////////////////////////": "",
    "lint the .js files in client": "",
    "lint:js": "eslint \"./client/**/*.js\" --fix",
    "//////////////////////////////////////         lint:css          ///////////////////////////////////": "",
    "lint the .css files in client": "",
    "lint:css": "stylelint \"./client/**/*.css\" --fix",
    "//////////////////////////////////////         apiProxy/Node     ///////////////////////////////////": "",
    "start a hapi server at localhost:8090": "",
    "apiProxy": "nodemon ./server/entry.js --ignore ./client/",
    "productionLocal:start": "NODE_ENV=test SIGNUP_API_PORT=8085 nodemon ./server/entry.js",
    "production:start": "NODE_ENV=production forever start ./server/entry.js",
    "production:stop": "NODE_ENV=production forever stopall",
    "staging:start": "NODE_ENV=staging forever start ./server/entry.js",
    "staging:stop": "NODE_ENV=staging forever stopall"
  },
  "contributors": [
    "Camilo Restrepo"
  ],
  "author": {
